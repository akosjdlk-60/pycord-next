{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>Pycord-Development/renovate-config",
    ":semanticPrefixFixDepsChoreOthers",
    ":dependencyDashboard",
    ":enablePreCommit"
  ],
  "forkProcessing": "enabled",
  "packageRules": [
    {
      "groupName": "GitHub Packages",
      "matchDatasources": ["github-tags","github-releases"],
      "automerge": false
    }
  ],
  "lockFileMaintenance": {
    "enabled": true
  }
}
